# 이메일 설정 (Gmail SMTP)
SMTP_HOST=smtp.gmail.com
SMTP_PORT=587
SMTP_USERNAME=your-email@gmail.com
SMTP_PASSWORD=your-app-password
FROM_EMAIL=noreply@blueprint.io
FROM_NAME=Blueprint

# SMS 설정 (Aligo)
SMS_PROVIDER=aligo
SMS_API_KEY=your-aligo-api-key
SMS_API_SECRET=your-aligo-user-id
